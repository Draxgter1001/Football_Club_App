<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FootBallClub10"
        tools:targetApi="31">
        <activity
            android:name=".SearchClubs"
            android:exported="false"
            android:label="@string/title_activity_search_clubs"
            android:theme="@style/Theme.FootBallClub10" />
        <activity
            android:name=".SearchClubsLeague"
            android:exported="false"
            android:label="@string/title_activity_search_clubs_league"
            android:theme="@style/Theme.FootBallClub10" />
        <activity
            android:name=".AddLeagues"
            android:exported="false"
            android:label="@string/title_activity_add_leagues"
            android:theme="@style/Theme.FootBallClub10" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.FootBallClub10">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>